@model List<Category>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html lang="en">

<body>

    <div class="content">
        <h3>Kategoriler</h3>


        <table class="table table-hover">
            <tr>
                <th>Id</th>
                <th>Kategori</th>
            </tr>
            @foreach (var item in Model)
            {

                <tr>
                    <td>@item.CategoryId</td>
                    <td>@item.CategoryName</td>
                </tr>

            }

        </table>

        <br />
    </div>
</body>
</html>